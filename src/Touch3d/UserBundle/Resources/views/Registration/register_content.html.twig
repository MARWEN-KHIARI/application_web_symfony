{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST"
      class="fos_user_registration_register form-signin">
    <h2 class="form-signin-heading">Please sign up</h2>
    {{ form_widget(form) }}
    <div>
        <input type="submit" class="btn btn-large btn-primary" value="{{ 'registration.submit'|trans }}"/>
    </div>
</form>
